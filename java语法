    int len = str.length();

    String str = "Hello";
        String substr = str.substring(2); // substr 现在是 "llo"
        String substr = str.substring(1, 4); // substr 现在是 "ell"
        int index = str.indexOf("l"); // index 现在是 2
        int lastIndex = str.lastIndexOf("l"); // lastIndex 现在是 3\
